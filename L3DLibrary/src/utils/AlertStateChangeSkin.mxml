<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
		alpha.disabled="0.2">
	<fx:Metadata>
		<![CDATA[ 
		/** 
		* @copy spark.skins.spark.ApplicationSkin#hostComponent
		*/
		[HostComponent("utils.AlertButton")]
		]]>
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="up"
				 stateGroups="normal"/>
		<s:State name="over"/>
		<s:State name="down"
				 stateGroups="normal"/>
		<s:State name="disabled"
				 stateGroups="normal"/>
	</s:states>
	
	<s:BitmapImage id="i1"
				   source="{hostComponent.sourceOver}"
				   visible.over="true"
				   visible.normal="false"/>
	<s:BitmapImage id="i2"
				   source="{hostComponent.sourceOut}"
				   visible.normal="true"
				   visible.over="false"/>
</s:Skin>